{% extends 'base.html.twig' %}

{% block title %}Partenaire{% endblock %}

{% block content %}
    <div class="wrapper">
        {% include('menuBackoffice.html.twig') %}
        <div class="container">
            <h1 class="pt-5 pb-5">Gestion du partenaire {{ partenaire.nom }}</h1>
            <table class="table">
                <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ partenaire.id }}</td>
                    </tr>
                    <tr>
                        <th>Nom</th>
                        <td>{{ partenaire.nom }}</td>
                    </tr>
                    <tr>
                        <th>UrlLogo</th>
                        <td>{{ partenaire.urlLogo }}</td>
                    </tr>
                </tbody>
            </table>
            <a href="{{ path('partenaire_index') }}">Liste</a>
            <a href="{{ path('partenaire_edit', {'id': partenaire.id}) }}"class="ml-3">Editer</a>
            {{ include('Backoffice/partenaire/_delete_form.html.twig') }}
        </div>
    </div>
{% endblock %}
